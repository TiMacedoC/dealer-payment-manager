generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Dealer {
  id        Int      @id @default(autoincrement())
  name      String
  cpf       String
  nickName  String?  @map("nick_name")
  birthDate DateTime @map("birth_date")
  pixKey    String   @map("pix_key")

  @@map("dealers")
}
